# Database
# Vercel Postgres에서 제공하는 pooled connection string
DATABASE_URL="postgres://username:password@host:port/database?pgbouncer=true&connect_timeout=15"

# Vercel Postgres에서 제공하는 direct connection string (마이그레이션용)
DIRECT_URL="postgres://username:password@host:port/database"

# Environment
NODE_ENV="production"

# Cron Job Security (선택사항)
# Vercel Cron Job에서 API 호출 시 인증에 사용
CRON_SECRET="your-random-secret-key-here"
